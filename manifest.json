{
  "manifest_version": 3,
  "name": "Malayalam Typing Judge",
  "version": "1.0",
  "description": "Plays Malayalam audio based on typing speed - Works on ALL websites!",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["audio/*"],
      "matches": ["<all_urls>"]
    }
  ]
}